<Window x:Class="TLAuto.Music.ServerHost.Dialog.EditMusicMarkDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="音乐标识符信息" Height="100" Width="300" Style="{StaticResource WindowIconStyleForDialog}">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto" />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <TextBlock Text="标识符：" VerticalAlignment="Center" Margin="10,0,0,0" />
        <ComboBox x:Name="CboMarks" Grid.Row="0" Grid.Column="1" VerticalAlignment="Center" Margin="0,0,10,0" />
        <Button Grid.Row="1" Grid.Column="1" IsDefault="True" Content="Ok" Width="70" VerticalAlignment="Center" Margin="0,0,10,0" HorizontalAlignment="Right" Click="OkButton_Click" />
    </Grid>
</Window>