<Window x:Class="TLAuto.Notification.ServerHost.Dialog.EditAppNameDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TLAuto.Notification.ServerHost.Dialog"
        mc:Ignorable="d"
        Title="填写App进程名字" Height="100" Width="300" Style="{StaticResource WindowIconStyleForDialog}">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto" />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <TextBlock Text="进程名：" VerticalAlignment="Center" Margin="10,0,0,0" />
        <TextBox Grid.Row="0" Grid.Column="1" x:Name="TxtProcessName" VerticalAlignment="Center" Margin="0,0,10,0" />
        <Button Grid.Row="1" Grid.Column="1" IsDefault="True" Content="Ok" Width="70" VerticalAlignment="Center" Margin="0,0,10,0" HorizontalAlignment="Right" Click="OkButton_Click" />
    </Grid>
</Window>