<Window x:Class="TLAuto.ControlEx.App.Dialogs.SelectMarkServiceAddressDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="选择标识符" Height="100" Width="300" Style="{StaticResource WindowIconStyleForDialog}">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto" />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <TextBlock Text="标识符：" VerticalAlignment="Center" Margin="10,0,0,0" />
        <ComboBox Grid.Row="0" Grid.Column="1" x:Name="CboMarks" VerticalAlignment="Center" Margin="0,0,10,0" />
        <Button Grid.Row="1" Grid.Column="1" Content="Ok" Width="70" VerticalAlignment="Center" Margin="0,0,10,0" HorizontalAlignment="Right" Click="OkButton_Click" IsDefault="True" />
    </Grid>
</Window>