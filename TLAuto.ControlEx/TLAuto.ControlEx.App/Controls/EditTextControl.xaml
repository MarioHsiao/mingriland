<UserControl x:Class="TLAuto.ControlEx.App.Controls.EditTextControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid VerticalAlignment="Center">
        <TextBlock x:Name="TblHeader" />
        <TextBox x:Name="TxtEditHeader" Loaded="TxtEditHeader_Loaded" Visibility="Collapsed" KeyDown="TxtEditHeader_KeyDown" LostFocus="TxtEditHeader_LostFocus" LostKeyboardFocus="TxtEditHeader_LostKeyboardFocus" />
    </Grid>
</UserControl>